{
    "manifestVersion": 1,
    "id": "AzureDevOpsWikiToDocFx", 
    "name": "Azure DevOps Wiki to DocFx",
    "version": "1.2.0",
    "publisher": "Monta",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "A task that generates a DocFX website out of your Azure DevOps wiki repository.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "icon.png"
    },
    "files": [
        {
            "path": "AzureDevOpsWikiToDocFx"
        }
    ],
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/Montapacking/AzureDevOpsWikiToDocFx"
    },    
    "galleryFlags": [
        "Public",
        "Preview"
    ],
    "contributions": [
        {
            "id": "AzureDevOpsWikiToDocFx",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "AzureDevOpsWikiToDocFx"
            }
        }
    ]
}